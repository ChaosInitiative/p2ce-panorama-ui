{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "standard",
		"prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
		"no-undef": "off",
		"no-ternary": "off",
		"arrow-parens": "error",
		"indent": "off",
		"no-mixed-spaces-and-tabs": "error",
		"curly": "off"
    }
}
